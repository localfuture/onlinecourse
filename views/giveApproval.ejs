<%- include('adminHeader'); %>
<h1>User List</h1>
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Email</th>
            <th scope="col">Approval</th>
            <th scope="col">Reply</th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody class="tbody-light">
        <% for(var i =0; i< data.length; i++){%>
        <tr>
            <td> <%= data[i].username %> </td>
            <td> <%= data[i].approval %> </td>
            <td> <%= data[i].reply %> </td>
            <td> <a href="/approve/<%=data[i]._id %>" class="btn btn-secondary btn-lg active" role="button"
                    aria-pressed="true">Approve</a></td>

        </tr>
        <%} %>
    </tbody>
</table>
<%- include('adminFooter'); %>